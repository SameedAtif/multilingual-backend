<% if @organization.errors.any? %>
  <p><%= @organization.errors.full_messages.to_sentence%></p>
<% end %>
<%= form_with model: @organization, local: true do |f| %>
  <div>
    <%= f.label :name %><br>
    <%= f.text_field :name%>
  </div>
  <div>
    <%= f.label :website %><br>
    <%= f.text_field :website %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
