<div class="flex min-h-screen">
  <%= render partial: 'organizations/settings_sidebar', locals: { active_tab: 'overview' } %>
  <main class="flex-1 px-8 py-2">
    <h1 class="text-2xl font-semibold mb-6">Overview</h1>
    <section class="space-y-6">
      <div>
        <h2 class="text-lg font-semibold">Organization settings</h2>
        <p class="text-sm text-muted-foreground">Manage your Organization preferences</p>
        <div class="grid grid-cols-1 gap-4 mt-4 md:grid-cols-2">
          <%= link_to edit_user_registration_path do %>
            <div class="rounded-lg border bg-card text-card-foreground shadow-sm p-4 hover:bg-gray-100 transition-colors" data-v0-t="card">
              <div class="flex items-center mb-2">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="h-5 w-5 mr-2 lucide lucide-user-round-cog"><path d="M2 21a8 8 0 0 1 10.434-7.62"/><circle cx="10" cy="8" r="5"/><circle cx="18" cy="18" r="3"/><path d="m19.5 14.3-.4.9"/><path d="m16.9 20.8-.4.9"/><path d="m21.7 19.5-.9-.4"/><path d="m15.2 16.9-.9-.4"/><path d="m21.7 16.5-.9.4"/><path d="m15.2 19.1-.9.4"/><path d="m19.5 21.7-.4-.9"/><path d="m16.9 15.2-.4-.9"/></svg>

                <h3 class="text-sm font-medium">Account settings</h3>
              </div>
              <p class="text-sm text-muted-foreground">Update your name and password</p>
            </div>
          <% end %>
          <%= link_to settings_organizations_path do %>
            <div class="rounded-lg border bg-card text-card-foreground shadow-sm p-4 hover:bg-gray-100 transition-colors" data-v0-t="card">
              <div class="flex items-center mb-2">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="h-5 w-5 mr-2 lucide lucide-building-2"><path d="M6 22V4a2 2 0 0 1 2-2h8a2 2 0 0 1 2 2v18Z"/><path d="M6 12H4a2 2 0 0 0-2 2v6a2 2 0 0 0 2 2h2"/><path d="M18 9h2a2 2 0 0 1 2 2v9a2 2 0 0 1-2 2h-2"/><path d="M10 6h4"/><path d="M10 10h4"/><path d="M10 14h4"/><path d="M10 18h4"/></svg>
                <h3 class="text-sm font-medium">Manage Organization</h3>
              </div>
              <p class="text-sm text-muted-foreground">Preview informaiton, set default chat assignee</p>
            </div>
          <% end %>
          <%= link_to integrations_organizations_path do %>
            <div class="rounded-lg border bg-card text-card-foreground shadow-sm p-4 hover:bg-gray-100 transition-colors" data-v0-t="card">
              <div class="flex items-center mb-2">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="h-5 w-5 mr-2 lucide lucide-blocks"><rect width="7" height="7" x="14" y="3" rx="1"/><path d="M10 21V8a1 1 0 0 0-1-1H4a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-5a1 1 0 0 0-1-1H3"/></svg>
                <h3 class="text-sm font-medium">Manage Integrations</h3>
              </div>
              <p class="text-sm text-muted-foreground">
                Preview plugin information
              </p>
            </div>
          <% end %>
        </div>
      </div>
      <div>
        <h2 class="text-lg font-semibold">Inbox settings</h2>
        <p class="text-sm text-muted-foreground">Personalize your Inbox experience</p>
        <div class="grid grid-cols-1 gap-4 mt-4 md:grid-cols-2">
          <%= link_to settings_notifications_path do %>
            <div class="rounded-lg border bg-card text-card-foreground shadow-sm p-4 hover:bg-gray-100 transition-colors" data-v0-t="card">
              <div class="flex items-center mb-2">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="h-5 w-5 mr-2">
                  <path d="M6 8a6 6 0 0 1 12 0c0 7 3 9 3 9H3s3-2 3-9"></path>
                  <path d="M10.3 21a1.94 1.94 0 0 0 3.4 0"></path>
                </svg>
                <h3 class="text-sm font-medium">Notifications</h3>
              </div>
              <p class="text-sm text-muted-foreground">Manage how you receive notifications</p>
            </div>
          <% end %>
          <%= link_to settings_messages_path do %>
            <div class="rounded-lg border bg-card text-card-foreground shadow-sm p-4 hover:bg-gray-100 transition-colors" data-v0-t="card">
              <div class="flex items-center mb-2">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="h-5 w-5 mr-2 lucide lucide-mail-question"><path d="M22 10.5V6a2 2 0 0 0-2-2H4a2 2 0 0 0-2 2v12c0 1.1.9 2 2 2h12.5"/><path d="m22 7-8.97 5.7a1.94 1.94 0 0 1-2.06 0L2 7"/><path d="M18 15.28c.2-.4.5-.8.9-1a2.1 2.1 0 0 1 2.6.4c.3.4.5.8.5 1.3 0 1.3-2 2-2 2"/><path d="M20 22v.01"/></svg>
                <h3 class="text-sm font-medium">Messages preferences</h3>
              </div>
              <p class="text-sm text-muted-foreground">Customize your message entry settings</p>
            </div>
          <% end %>
        </div>
      </div>
      <div>
        <h2 class="text-lg font-semibold">Customer chat settings</h2>
        <p class="text-sm text-muted-foreground">
          Advanced settings for account and messaging capabilities managed in admin
        </p>
        <div class="grid grid-cols-1 gap-4 mt-4 md:grid-cols-2">
          <%= link_to appearance_setting_rooms_path do %>
            <div class="rounded-lg border bg-card text-card-foreground shadow-sm p-4 hover:bg-gray-100 transition-colors" data-v0-t="card">
              <div class="flex items-center mb-2">
                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="h-5 w-5 mr-2">
                  <path d="m9.06 11.9 8.07-8.06a2.85 2.85 0 1 1 4.03 4.03l-8.06 8.08"></path>
                  <path d="M7.07 14.94c-1.66 0-3 1.35-3 3.02 0 1.33-2.5 1.52-2 2.02 1.08 1.1 2.49 2.02 4 2.02 2.2 0 4-1.8 4-4.04a3.01 3.01 0 0 0-3-3.02z"></path>
                </svg>
                <h3 class="text-sm font-medium">Chat appearance</h3>
              </div>
              <p class="text-sm text-muted-foreground">Customize your online store chat widget</p>
            </div>
          <% end %>
          <%# <div class="rounded-lg border bg-card text-card-foreground shadow-sm p-4 hover:bg-gray-100 transition-colors" data-id="53" data-v0-t="card">
            <div class="flex items-center mb-2" data-id="54">
              <svg data-id="55" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="h-5 w-5 mr-2">
                <circle cx="12" cy="12" r="10"></circle>
                <polyline points="12 6 12 12 16 14"></polyline>
              </svg>
              <h3 class="text-sm font-medium" data-id="56">Availability</h3>
            </div>
            <p class="text-sm text-muted-foreground" data-id="57">Specify the first reply customers automatically get, based on your availability</p>
          </div> %>
        </div>
      </div>
    </section>
  </main>
</div>
